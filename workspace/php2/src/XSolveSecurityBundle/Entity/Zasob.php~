<?php

namespace XSolveSecurityBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Zasob
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="XSolveSecurityBundle\Entity\ZasobRepository")
 */
class Zasob
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

   /**
     * @var string
     *
     * @ORM\Column(name="name", type="text")
     */
    protected $name;

   /**
    * @ORM\ManyToOne(targetEntity="User", inversedBy="zasoby")
    * @ORM\JoinColumn(name="user_id", referencedColumnName="id")
    * */
   protected $user;

   /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return Zasob
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }
}
